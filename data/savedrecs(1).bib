@article{ ISI:000359703800002,
Author = {Guelzo, Allen C.},
Title = {{Lincoln, Cobden, and Bright: The Braid of Liberalism in the Nineteenth
   Century's Transatlantic World}},
Journal = {{AMERICAN POLITICAL THOUGHT}},
Year = {{2015}},
Volume = {{4}},
Number = {{3}},
Pages = {{391-411}},
Month = {{SUM}},
Abstract = {{Abraham Lincoln is usually described as a purely American, almost
   provincial, figure. But his ideas and wartime policies should also be
   understood in a transatlantic context, since Lincoln's ideas and
   policies conform so remarkably to the patterns of classical liberal
   ideas in Europe in the nineteenth century, and especially to the
   Manchester School of Richard Cobden and John Bright. In contrast to the
   failures and disappointments of Continental liberalism, Lincoln's
   triumph in the American Civil War represents a world historical victory
   for bourgeois liberal democracy.}},
