@article{ ISI:000351632700008,
Author = {Ickes, Charlotte},
Title = {{The Sartorial and the Skin Portraits of Pocahontas and Allegories of
   English Empire}},
Journal = {{AMERICAN ART}},
Year = {{2015}},
Volume = {{29}},
Number = {{1}},
Pages = {{82-105}},
Month = {{SPR}},
Abstract = {{In 1616, Dutch engraver Simon van de Passe created a portrait of
   Pocahontas. Dressed in Jacobean garb, Pocahontas's clothing visualized
   the sitter's newly acquired English status and her royal New World
   lineage. Through the surface of clothing, Pocahontas's body transforms
   into a heraldic symbol of her hybrid pedigree. Over a century later, an
   unknown artist rendered the print in paint. Obscuring the earlier
   portrait's transatlantic sartorial surface, the painted likeness focuses
   less on Pocahontas's dress and more on her notably whitened skin. This
   essay examines how the particulars of portraiture and the abstraction of
   allegory met on the ground of the sartorial and then the skin to both
   reflect and resist English imperialism.}},
