@article{ ISI:000357344600001,
Author = {Jurvelin, Heidi and Jokelainen, Jari and Takala, Timo},
Title = {{Transcranial Bright Light and Symptoms of Jet Lag: A Randomized,
   Placebo-Controlled Trial}},
