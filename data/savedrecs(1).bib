@article{ ISI:000361915600004,
Author = {Tang, Donny},
Title = {{Has the European Monetary Union Promoted the European Union
   High-Technology Investments in the United States?}},
Journal = {{JOURNAL OF APPLIED ECONOMICS AND BUSINESS RESEARCH}},
Year = {{2015}},
Volume = {{5}},
Number = {{3}},
Pages = {{171-193}},
Abstract = {{This study examines whether the European Economic and Monetary Union (
   EMU) has promoted the European Union (EU) high-technology foreign direct
   investment (FDI) in the U.S. during 1993-2012. The results indicate that
   the higher bank credit flows have substantially boosted the FDI for the
   entire period. The results also indicate that the higher stock market
   capitalization has modestly boosted the FDI during the pre-EMU period.
   The results have very important implications for the U.S. policies
   toward the EU FDI. Given the U.S. global technological lead, the U.S.
   can attract more EU FDI inflows by promoting the transatlantic research
   collaborations. Copyright (C) 2015 JAEBR}},
