@inproceedings{ ISI:000410788000082,
Author = {Peter, Terem and Marek, Lenc},
Editor = {{Bittmanova, B and Vlkova, E}},
Title = {{THE SIGNIFICANCE OF ALLIANCES AND THE CURRENT HEADING OF THE EUROPEAN
   FOREIGN POLICY OF THE SLOVAK REPUBLIC}},
Booktitle = {{INTERNATIONAL RELATIONS 2015: CURRENT ISSUES OF WORLD ECONOMY AND
   POLITICS}},
Year = {{2015}},
Pages = {{674-686}},
Note = {{16th International Scientific Conference on International Relations -
   Current Issues of World Economy and Politics, Smolenice, SLOVAKIA, DEC
   03-04, 2015}},
Organization = {{Univ Econ Bratislava, Fac Int Relat}},
Abstract = {{The current security situation in the imminent neighbourhood of the
   Slovak Republic, recent developments in the Middle East and the constant
   flow of migrants to particular countries of the European Union create
   new and major challenges for the European dimension of the Slovak
   foreign policy. At the moment, the Slovak republic has to reflect and
   reflects the international affairs in its closer as well as wider
   neighbourhood also through the mechanism of Common Foreign and Security
   Policy and creates corresponding position in the dynamically changing
   environment in dialogue with its transatlantic and European allies.
   Therefore, the current situation primarily represents a major foreign
   policy test for the diplomacy and foreign policy of the Slovak Republic
   that will verify the unity and synergy of our positions in broader
   alliance and EU dimensions. This three mentioned regional problems with
   global impact significantly affect the position of the Slovak Republic
   in the framework of the regional (V4) and European level (EU) and also
   create significant implications for new perception of promotion of
   national interests of the Slovak Republic in the eyes of its European
   and transatlantic allies. At the same time the quality of dialogue at
   European level in the context of the current challenges may have
   significant implications for priorities, as well as effective
   performance of the Slovak Presidency of the Council of the EU in the
   second half of the year of 2016.}},
