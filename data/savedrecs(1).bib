@article{ ISI:000359057300014,
Author = {Teresa Sanhueza, Maria},
Title = {{ITALIAN IMMIGRANTS IN ARGENTINA: THE LETTERS OF ORESTE, ABELE AND LUIGI
   SOLA (1901-1922)}},
Journal = {{ZIBALDONE-ESTUDIOS ITALIANOS DE LA TORRE DEL VIRREY}},
Year = {{2015}},
Volume = {{3}},
Number = {{1}},
Pages = {{183-205}},
Month = {{JAN}},
Abstract = {{Immigration is usually analyzed by the study of data and numbers that
   explain the behaviour of the masses but do not mention individual
   experiences. That is why the epistolary correspondence of the Sola
   family, exchanged without interruption through twenty-two years, 1901 to
   1922, should be seen not only as a historical document but also in its
   social and political dimensions. It provides a snapshot of the immigrant
   experience through bi-directional communication between members of a
   family and it shows a transnational perspective on how immigration
   transformed Italy and Argentina, that is to say, considered as a public
   discourse, as well as a private discourse.}},
