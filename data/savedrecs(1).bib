@article{ ISI:000353712400002,
Author = {Moreda Rodriguez, Eva},
Title = {{Transatlantic Networks in the Correspondence of Two Exiled Spanish
   Musicians, Julian Bautista and Adolfo Salazar}},
Journal = {{JOURNAL OF THE ROYAL MUSICAL ASSOCIATION}},
Year = {{2015}},
Volume = {{140}},
Number = {{1}},
Pages = {{93-119}},
Month = {{JAN 2}},
Abstract = {{Studies of the Spanish Republican exile, both musicological and
   otherwise, have often worked under the assumption that the exiles were
   disconnected from Francoist Spain and were thus unable to contribute in
   any way to the musical life of their home country. This article
   re-examines these assumptions by analysing a hitherto unexplored corpus
   of correspondence between two exiled musicians, Julian Bautista and
   Adolfo Salazar, and other musicians who had stayed in Francoist Spain.
   Such correspondence suggests that the exiles could, and indeed did,
   contribute to Spanish musical life under Francoism in a variety of ways.}},
