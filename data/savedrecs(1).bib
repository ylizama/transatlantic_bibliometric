@article{ ISI:000377632800003,
Author = {Schaufelbuehl, Janick Marina},
Title = {{The transatlantic business community faced with US direct investment in
   Western Europe, 1958-1968}},
Journal = {{BUSINESS HISTORY}},
Year = {{2016}},
Volume = {{58}},
Number = {{6}},
Pages = {{880-902}},
Abstract = {{This article examines the position of US and European business in the
   debate about American direct investment in Western Europe in a
   historical perspective, from the establishment of the Common Market to
   the introduction of US regulation of foreign direct investment (FDI) a
   decade later. Based on abundant and diverse archival documents, it sheds
   new light on the process of Americanisation and contributes to existing
   research on transnational networks, by revealing the active role
   industrial leaders on both sides of the Atlantic played in shaping the
   political responses to problems raised by the American firms' massive
   presence in the Common Market.}},
