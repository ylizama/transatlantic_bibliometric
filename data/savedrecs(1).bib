@article{ ISI:000364731900003,
Author = {Yankholmes, Aaron and McKercher, Bob},
Title = {{Understanding visitors to slavery heritage sites in Ghana}},
Journal = {{TOURISM MANAGEMENT}},
Year = {{2015}},
Volume = {{51}},
Pages = {{22-32}},
Month = {{DEC}},
Abstract = {{Previous research has touched on the dissonance in visitors' experiences
   at sites associated with the Transatlantic Slave Trade (TAST) in Ghana,
   without considering the influence of their personal connection to the
   slavery and trip motives. Based on a survey of 550 visitors, this paper
   profiles visitors and then examines their knowledge and attitudes
   towards the site, motives for visitation and sensitivity towards other
   visitors. Four visitor segments were identified; connected slavery
   heritage visitor, connected vacationer, not connected bicultural and not
   connected Caucasian. Significant differences were found among the four
   cohorts with regard to their demographic and trip profiles as well as
   motives and knowledge of the site. However, as the paper describes,
   understanding visitor behaviour at such places depends on the intensity
   of experience sought or gained. The paper thus argues that care should
   be taken in distinguishing between the quintessential needs of root
   seekers from pleasure seekers. (C) 2015 Elsevier Ltd. All rights
   reserved.}},
