Title = {{Comparison of Two Continuous GPS Carrier-Phase Time Transfer Techniques}},
Booktitle = {{2015 JOINT CONFERENCE OF THE IEEE INTERNATIONAL FREQUENCY CONTROL
   SYMPOSIUM \& THE EUROPEAN FREQUENCY AND TIME FORUM (FCS)}},
Series = {{IEEE International Frequency Control Symposium}},
