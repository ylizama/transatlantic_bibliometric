@article{ ISI:000326212100018,
Author = {Trapp, Monika and Wewel, Claudio},
Title = {{Transatlantic systemic risk}},
Journal = {{JOURNAL OF BANKING \& FINANCE}},
Year = {{2013}},
Volume = {{37}},
Number = {{11}},
Pages = {{4241-4255}},
Month = {{NOV}},
Abstract = {{In this paper we study systemic risk for the US and Europe. We show that
   banks' exposures to common risk factors are crucial for systemic risk.
   We come to this conclusion by first showing that relations between US
   and European banks are smaller than within each region. We then show
   that European banks react more strongly to the onset of the financial
   crisis than US banks. Regarding the consequences of systemic risk, we
   show that dependence between the banking sector and a wide range of real
   sectors is limited. Our results imply that regulators and supervisors
   should address international bank dependencies arising from common risk
   factors, while recessions in real sectors due to bank defaults should be
   a secondary concern. (C) 2013 Elsevier B.V. All rights reserved.}},
