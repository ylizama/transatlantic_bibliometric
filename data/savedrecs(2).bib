@article{ ISI:000331337100010,
Author = {Perreau, Bruno},
Title = {{THE QUEER MIND THROUGH THE TRANSATLANTIC LOOKING-GLASS1}},
Journal = {{CONTEMPORARY FRENCH AND FRANCOPHONE STUDIES}},
Year = {{2014}},
Volume = {{18}},
Number = {{1, SI}},
Pages = {{75-85}},
Month = {{JAN 1}},
Abstract = {{Queer theory, originating in the U.S. in the 1990s, adopted much of its
   conceptual apparatus from so-called French theory. In retrospect we can
   ask what need was being fulfilled by the creation of these two bodies of
   thought, which included some very heterogeneous thinkers. The article
   examines the mechanisms of cultural translation by studying the
   epistemic relationship between French theory and queer theory.
   Specifically it demonstrates how Foucault's ``associative{''} thought
   formed an essential resource for deconstructing sexual categories and
   reviving the critical potential of language on both sides of the
   Atlantic.}},
