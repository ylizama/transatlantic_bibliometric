@article{ ISI:000308297600004,
Author = {Alexandru, Maria-Sabina Draga},
Title = {{Transatlantic translations as identity mediating discourses}},
Journal = {{PERSPECTIVES-STUDIES IN TRANSLATOLOGY}},
Year = {{2012}},
Volume = {{20}},
Number = {{3, SI}},
Pages = {{297-311}},
Abstract = {{This paper discusses Oana Durican's translation into Romanian of the
   Romanian-American writer Domnica Radulescu's novel Train to Trieste and
   Andrei Codrescu's translation into English of Forgiven Submarine, the
   poem he wrote collaboratively with Ruxandra Cesereanu. Starting from the
   idea that translation is a form of experimental writing, I analyse the
   ways in which these authors adapt linguistic and conceptual mindsets
   across the Atlantic, thus contributing in various ways to the
   translation/relocation of their identities across cultures. I will show
   that these stories of border-crossing, which are centred on the two
   conceptual metaphors of the train and of the submarine, respectively,
   both strive to construct coherent narratives about a traumatic
   historical past, a dimension that is further highlighted in the process
   of translation.}},
