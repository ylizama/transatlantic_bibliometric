@article{ ISI:000329142400002,
Author = {Reily, Suzel Ana},
Title = {{Reconceptualizing ``Musical Mulatismo{''} in the Mining Regions of
   Portuguese America}},
Journal = {{WORLD OF MUSIC-NEW SERIES}},
Year = {{2013}},
Volume = {{2}},
Number = {{2}},
Pages = {{25-46}},
Abstract = {{In the 1940s the German-Uruguayan musicologist Francisco Curt Lange
   (1903-1997) published a preliminary report containing his findings on
   colonial music in the Portuguese American mining regions of Minas
   Gerais, Brazil. He noted that most professional musicians operating in
   the region during the 18th century were mulatos, a phenomenon he called
   ``musical mulatismo. ``In contrast to the reigning nationalist
   discourses in Brazil, in which the nation was being constructed around
   hybridities, the mulatos in Minas engaged in locally composed liturgical
   repertoires in a marked European pre-Classical style. Lange's findings,
   therefore, did not only call for revisions to the narratives on
   Brazilian Art Music, they would also require a reconceptualization of
   musical mulatismo. This paper assesses these debates in light of Lange's
   contributions to current understanding of the mulato experience in
   Portuguese America and of how musical activities that followed the
   prevailing trend in Europe came to be used by them as a means of
   confronting the complex transatlantic encounters in the colonial
   setting.}},
