@article{ ISI:000329142400004,
Author = {Dettmann, Christine},
Title = {{History in the Making: An Ethnography into the Roots of Capoeira Angola}},
Journal = {{WORLD OF MUSIC-NEW SERIES}},
Year = {{2013}},
Volume = {{2}},
Number = {{2}},
Pages = {{73-98}},
Abstract = {{By focusing on the history-making of the Afro-Brazilian martial art
   capoeira, challenges for research into the African Diaspora become
   apparent. By outlining the research history to date of Capoeira Angola
   within the academic realm of African American Studies, the contentious
   territory between Afrocentric and creolizationist ideas comes to the
   fore. Here, the agents of capoeira history-making extend far across
   national borders and continents, stretching beyond the boundaries of the
   lusophone world. This is particularly noticeable when tracing the
   historic trail of the Angolan combat game engolo, rumoured to be an
   African ancestor of Capoeira Angola and as such, since the 1970s, a
   powerful signifier for capoeira practitioners. My concluding remarks'
   allude to the increasing intersection between scholars and practitioners
   involved in this global history-making process. It is therefore
   necessary to develop an ethnography sensitized to the identity politics
   of history-making in order to better ground and position future
   transatlantic research in ethnomusicology.}},
