@article{ ISI:000303297900004,
Author = {Squibbs, Richard},
Title = {{Lawyers, Republicanism, and the Federalist Periodical Essay in The Port
   Folio}},
Journal = {{HUNTINGTON LIBRARY QUARTERLY}},
Year = {{2012}},
Volume = {{75}},
Number = {{1}},
Pages = {{69-93}},
Abstract = {{This essay explains how Federalist lawyer-writers adapted the British
   periodical essay to their own circumstances in Philadelphia and revised
   the putatively topical genre into a literary form of historical witness.
   Richard Squibbs draws on a variety of essays, letters, and other
   writings from the period to show how the pressures of Jeffersonian
   populism compelled these Federalist writers to concede their literary
   attempts at civic and cultural reform to posterity. The essay also
   demonstrates the centrality of Ciceronian humanism to the Federalist
   conception of the periodical essay genre, and to the turn the Port Folio
   essayists made from direct political engagement to recording their
   failed efforts for the benefit of future readers and polities.}},
