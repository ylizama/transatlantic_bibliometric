@article{ ISI:000341407500006,
Author = {Pedaliu, Effie G. H.},
Title = {{Transatlantic Relations at a Time When `More Flags' Meant `No European
   Flags': the United States' War in South-East Asia and its European
   Allies, 1964-8}},
Journal = {{INTERNATIONAL HISTORY REVIEW}},
Year = {{2013}},
Volume = {{35}},
Number = {{3}},
Pages = {{556-575}},
Abstract = {{The article discusses why and how the United States and its North
   Atlantic Treaty Organization (NATO) European allies came to hold such
   divergent opinions over what was at stake in Vietnam. It also examines
   how European reluctance to fight alongside the United States in
   South-East Asia affected relations within NATO. The voices of the
   smaller NATO members will also be considered. The article concludes that
   the transatlantic rift was contained not just because of cold-war
   realities but also because the Johnson and Nixon administrations took
   steps to repair the transatlantic relationship and because European
   integration acted as a binding force.}},
