@article{ ISI:000313766400001,
Author = {Carrere, Marissa},
Title = {{``LET THEM SINK INTO OBLIVION{''}: GENEALOGICAL FORM AND FAMILIAL
   FORGETTING IN SUSANNA ROWSON'S REUBEN AND RACHEL}},
Journal = {{EIGHTEENTH-CENTURY STUDIES}},
Year = {{2013}},
Volume = {{46}},
Number = {{2}},
Pages = {{183+}},
Month = {{WIN}},
Abstract = {{Marissa Carrere, ``Let Them Sink Into Oblivion{''}: Genealogical Form
   and Familial Forgetting in Susanna Rowson's Reuben and Rachel This essay
   examines thematic and structural ``forgetting{''} in Susanna Rowson's
   1798 Reuben and Rachel. As the novel moves closer to the
   postrevolutionary era of its publication, its second volume shifts
   abruptly from genealogy to sentimental romance, and its eponymous
   characters actively forget the transatlantic, hemispheric, and hybrid
   ancestry that the novel heretofore existed to construct and that would
   otherwise destabilize their emergent national identity. I locate Reuben
   and Rachel's sibling intimacy at the heart of the contradiction between
   the second volume's forgetfulness and both the first volume's and
   Rowson's stated authorial commitments to the transmission of historical
   memory.}},
