@article{ ISI:000260616600007,
Author = {Wilson, Christopher P.},
Title = {{Lost Boys and Recovered Classics: Literary and Social Memory in Lorenzo
   Carcaterra's Sleepers (1995)}},
Journal = {{JOURNAL OF AMERICAN STUDIES}},
Year = {{2008}},
Volume = {{42}},
Number = {{1, SI}},
Pages = {{107-131}},
Month = {{APR}},
Abstract = {{This essay explores the interplay of literary and social memory in
   Lorenzo Carcaterra's prison and revenge memoir, Sleepers (199 5), in
   particular its appropriation of Alexander Dumas's The Count of Monte
   Cristo (1844-45) as both a literary template and a political marker.
   Monte Cristo is the central text in Carcaterra's rewriting of a long
   tradition of ``bad-boy{''} narratives depicting juvenile dispossession,
   crime, and redemptive vigilantism. The essay also traces the
   transatlantic routes of Dumas in modern US popular culture, especially
   in Albert Kanter's Illustrated Classics, in fight of Antonio Gramsci's
   theories Of Popular narrative and law.}},
