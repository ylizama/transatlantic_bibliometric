@article{ ISI:000244091200006,
Author = {Klemek, Christopher},
Title = {{Placing Jane Jacobs within the transatlantic urban conversation}},
Journal = {{JOURNAL OF THE AMERICAN PLANNING ASSOCIATION}},
Year = {{2007}},
Volume = {{73}},
Number = {{1}},
Pages = {{49-67}},
Month = {{WIN}},
Abstract = {{After making a comparative historical survey of contemporaneous
   responses to her 1964 book, The Death and Life of Great American Cities,
   Klemek concludes that Jane Jacobs' ideas were anticipated by urbanists
   in Great Britain and West Germany, and were embraced more readily by
   influential planners and architects in these nations and Canada than was
   initially the case in the United States. Unlike the almost reactionary
   outsider and opposition figure she seemed to be in New York, Jacobs was
   accepted by many types of planners and architects in these other
   nations, illustrating that her ideas were not as inherently
   antiplanning, antimodernist, or NIMBYist, as they appeared in the
   context of the United States.}},
