@article{ ISI:000257021000004,
Author = {Baylis, John},
Title = {{The 1958 Anglo-American Mutual Defence Agreement: The search for nuclear
   interdependence}},
Journal = {{JOURNAL OF STRATEGIC STUDIES}},
Year = {{2008}},
Volume = {{31}},
Number = {{3}},
Pages = {{425-466}},
Month = {{JUN}},
Abstract = {{Written on the 50th anniversary, this article focuses on the
   negotiations between Britain and the United States which led to the 1958
   Mutual Defence Agreement and the beginning of the `special nuclear
   relationship' which has lasted down to the present day. It is argued
   that the eventual success of the negotiations had a lot to do with the
   key roles of Eisenhower and Macmillan but that a transatlantic `advocacy
   coaltion' of nuclear scientists, defence and intelligence officials also
   played an important part at the operational level in achieving and
   subsequently shaping the kind of relationship which developed. Attention
   is also given to the longer term significance of the agreement,
   especially in terms of the arguments about its impact on nuclear
   proliferation.}},
