@inproceedings{ ISI:000240055000055,
Author = {Ramjoue, Celina},
Editor = {{Kaiser, M and Lien, ME}},
Title = {{The transatlantic rift in genetically modified food policy}},
Booktitle = {{Ethics and the Politics of Food}},
Year = {{2006}},
Pages = {{334-338}},
Note = {{6th Congress of the European-Society-for-Agricultural-and-Food-Ethics,
   Oslo, NORWAY, JUN 22-24, 2006}},
Organization = {{European Soc Agr \& Food Ethics; Forskningsetiske Komiteer; Univ Oslo;
   Res Council Norway; Fishery \& Aquaculture Ind Res Fund; Fdn Res Levy
   Agr Products; City Oslo}},
Abstract = {{The regulatory structures underlying United States and European Union
   policies regarding genetically modified food (GM) and crops are
   fundamentally different. The US regulates GM foods and crops as end
   products, applying the same regulatory framework that it would to non GM
   foods or crops. The EU, on the other hand, regulates agricultural
   biotechnology products as the result of a specific production process
   and has developed a wide network of rules that regulate GM foods and
   crops specifically. The result is a relatively permissive US regulation
   of GM foods, and a relatively restrictive regulation in the EU. Why are
   genetically modified food policies in the United States and the European
   Union different? This paper argues that differences between GM food
   policies in the US and the EU can be explained by three main elements.
   First, US and EU policies of the 1970s and 1980s on recombinant DNA
   research and events leading up to early GM food and crop regulation
   allow a deeper understanding of current policy. Second, underlying
   beliefs and values condition the content of current GM food and crop
   policy. Third, actors for and against agricultural biotechnology have
   achieved different levels of success in influencing policy in the US and
   the EU.}},
