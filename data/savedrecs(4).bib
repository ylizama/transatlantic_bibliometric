@article{ ISI:000228011600008,
Author = {Rudolf, P},
Title = {{The myth of the `German way': German foreign policy and transatlantic
   relations}},
Journal = {{SURVIVAL}},
Year = {{2005}},
Volume = {{47}},
Number = {{1}},
Pages = {{133+}},
Month = {{SPR}},
Note = {{Conference on German Foreign Policy in the 1990s and Beyond, Bruhl,
   GERMANY, JUL 14-17, 2004}},
Abstract = {{Those critics who warn of a new German unilateralism have read too much
   into the Iraq crisis. The core components of Germany's traditional
   foreign policy conception include a general strategic preference for
   embedding German foreign policy into multilateral frameworks; the goal
   of a civilised international order; and a preference for non-military
   means and strong aversion to the use of military force. German policies
   regarding the Iraq war may have been at odds with one or more of these
   core components; however, there were cross-cutting pressures that made
   it very difficult to be entirely faithful to those traditions. Neither
   the foreign-policy discourse in Germany with respect to the
   transatlantic relationship nor actual policies in the wake of the Iraq
   crisis indicate a profound change in the orientation of German foreign
   policy. But we can expect the strains of further adjustment and
   non-adjustment to a changing transatlantic framework.}},
