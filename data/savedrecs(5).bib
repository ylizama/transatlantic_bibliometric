@article{ ISI:000165771200002,
Author = {Cozzani, I and Giordano, S},
Title = {{On the use of sampling theory for passively monitoring QoS in packet
   networks fed by different types of traffic}},
Journal = {{EUROPEAN TRANSACTIONS ON TELECOMMUNICATIONS}},
Year = {{2000}},
Volume = {{11}},
Number = {{5}},
Pages = {{453-470}},
Month = {{SEP-OCT}},
Abstract = {{Performance measurements are a hot research topic for the design and
   control of high speed heterogeneous networks, such as the BISDN as well
   as the Internet. Our research in this field is concerned with a novel
   measurement system, with real-time monitoring capabilities, whose
   working is based on sampling survey. The scenario used to test the
   accuracy of the proposed methodology is a ``Passive QoS 2-point
   measurement system{''} acting on an ATM network, which is fed with
   different kinds of traffic: synthetic traces (traditional as well as LRD
   models) and a trace of real ATM traffic captured on a transatlantic
   UK-US link. Delay and loss characteristics, evaluated with the proposed
   sampling technique are compared against those foreseen by the sampling
   theory for several values of the sampling rate. The final result is a
   quantitative analysis that trades-off accuracy and implementation costs
   of the measurement instruments and is a powerful control tool usable by
   networks managers to check the quality of service they are providing, to
   understand network internal dynamics and to design their monitoring
   systems.}},
