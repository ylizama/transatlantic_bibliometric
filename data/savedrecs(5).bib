@article{ ISI:000186479800002,
Author = {Matlary, JH},
Title = {{Crisis management in the EU: More crisis than management?}},
Journal = {{INTERNASJONAL POLITIKK}},
Year = {{2003}},
Volume = {{61}},
Number = {{3}},
Pages = {{303+}},
Abstract = {{The author analyses EU actor capacity in security policy with special
   emphasis on crisis management. She places this within the larger context
   of security policy and the transatlantic debate over the role of NATO,
   the case of Iraq, and different national interests of key member states.
   The author concludes that the EU has steadily developed its capacity to
   undertake the various types of crisis management, but that the role of
   the EU as a security actor will be determined by the larger systemic
   decision-making in this area. The crisis over Iraq may very well lead to
   a consolidation of the EU's role.}},
