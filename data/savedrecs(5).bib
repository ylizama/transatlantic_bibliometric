@article{ ISI:000087189100003,
Author = {Haines, R and Shlomowitz, R},
Title = {{Explaining the mortality decline in the eighteenth-century British slave
   trade}},
Journal = {{ECONOMIC HISTORY REVIEW}},
Year = {{2000}},
Volume = {{53}},
Number = {{2}},
Pages = {{262+}},
Month = {{MAY}},
Abstract = {{The authors' quantitative analysis of the W.E.B. Du Bois Institute
   database of slave voyages confirms the mortality decline in the British
   slave trade during the eighteenth century. They scrutinize competing
   hypotheses for this decline, and explore the development and diffusion
   of empirical knowledge about disease prevention. Emphasis is placed on
   the implementation of life-saving regimes by maritime surgeons and the
   ramifications of increasing state intervention for non-slave voyages and
   civilian populations on land. Seeking context, the authors review the
   parallel mortality decline in England during the eighteenth century, and
   its arrest for about 50 years from 1820.}},
