@article{ ISI:000085951000004,
Author = {Grubb, F},
Title = {{The transatlantic market for British convict labor}},
Journal = {{JOURNAL OF ECONOMIC HISTORY}},
Year = {{2000}},
Volume = {{60}},
Number = {{1}},
Pages = {{94-122}},
Month = {{MAR}},
Abstract = {{Convicts account for at least one-quarter of British migration to
   mid-eighteenth-century America. Their transportation to and disposal in
   America was essentially an experiment in privatizing post-trial criminal
   justice, A model of this trade is developed that yields testable
   implicational regarding the relative distributional moments of convict
   auction prices, the size: of shipper profits, and how convicts were
   selected for transportation. Quantitative evidence is assembled to test,
   these implications and to measure the labor value of criminality. The
   results support the model and are used to explain the political economy
   of British penal policy,}},
