@article{ ISI:000176602400006,
Author = {Scott-Smith, G},
Title = {{The Congress for Cultural Freedom, the end of ideology and the 1955
   Milan Conference, `Defining the Parameters of Discourse'}},
Journal = {{JOURNAL OF CONTEMPORARY HISTORY}},
Year = {{2002}},
Volume = {{37}},
Number = {{3}},
Pages = {{437-455}},
Month = {{JUL}},
Abstract = {{The Congress of Cultural Freedom was founded in 1950 to gather together
   an international caucus of intellectuals to opposed totalitarianism in
   general and the influence of communism and neutralism in particular.
   Funded by the CIA, it was initially relatively successful in
   establishing itself in the western world, but then faced a crisis of
   purpose and direction following the death of Stalin in 1953 and the
   waning of the `cultural Cold War'. The conference held in Milan in 1955
   under the title `The Future of Freedom' marked a turning-point for the
   Congress. It was there that this organization became associated with and
   `end-of-ideology' discourse that provided significant perspectives on
   contemporary socio-economic and political conditions. This article
   offers an overview of this discourse, its main protagonists and the
   importance of the Milan conference in terms of the scope and direction
   of transatlantic intellectual connections in the 1950s.}},
