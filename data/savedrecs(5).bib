@article{ ISI:000172603000001,
Author = {May, AN and Phillips, J},
Title = {{Homicide in Nova Scotia, 1749-1815}},
Journal = {{CANADIAN HISTORICAL REVIEW}},
Year = {{2001}},
Volume = {{82}},
Number = {{4}},
Pages = {{625-661}},
Month = {{DEC}},
Abstract = {{This study of homicide in eighteenth- and early nineteenth-century Nova
   Scotia, based on prosecuted cases, reveals two distinct patterns. In
   Halifax the homicide rate was very high compared with most other
   transatlantic communities in approximately the same period, a phenomenon
   substantially attributable to the consistent presence of naval and
   military forces in the city. Elsewhere in Nova Scotia, homicide was a
   much less frequent occurrence. The nature of homicide was also different
   in the capital from the rest of the colony, In Halifax, people were more
   likely to kill strangers and casual acquaintances; in other communities,
