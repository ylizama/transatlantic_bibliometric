Author = {Kurtul, Hande and Akkartal, Erkut},
Editor = {{Bilgin, MH and Danis, H and Demir, E and Can, U}},
Title = {{Challenges and Possible Consequences of Transatlantic Trade and
   Investment Partnership}},
