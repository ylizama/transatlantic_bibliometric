@article{ ISI:000385817200002,
Author = {Bendtsen, Lasse},
Title = {{Domestic slave trading in St. Croix, 1764-1848}},
Journal = {{SCANDINAVIAN JOURNAL OF HISTORY}},
Year = {{2016}},
Volume = {{41}},
Number = {{4-5, SI}},
Pages = {{495-515}},
Month = {{SEP-DEC}},
Abstract = {{This article deals with domestic slave trading in the Danish
   (-Norwegian) West Indian colony of St. Croix, focusing on enslaved
   labourers sold as chattel. It examines the volume and price
   developments, building upon data on thousands of sales. Crucian
   slaveholders were willing participants in the domestic slave market,
   especially during the market's peak period, c. 1767-1823. Some of them
   did make a profit from speculation, although evidence of large-scale,
   professional entrepreneurs is absent. The closing of the transatlantic
   slave trade did not lead to rising slave prices in the short term, but
   in the long run it was an important factor in the decline of the market.
   Finally, the article discusses the impact of the market on enslaved
   Crucians and their masters, as well as their respective strategies for
   dealing with it.}},
