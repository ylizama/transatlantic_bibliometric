@article{ ISI:000372187600004,
Author = {Anagnostakis, Dimitrios},
Title = {{Securing the Transatlantic Maritime Supply Chains from Counterterrorism:
   EU-U.S. Cooperation and the Emergence of a Transatlantic Customs
   Security Regime}},
