@article{ ISI:000414357900021,
Author = {Gladyshev, S. V. and Gladyshev, V. S. and Gulev, S. K. and Sokov, A. V.},
Title = {{Subpolar mode water classes in the northeast Atlantic: Interannual and
   long-term variability}},
Journal = {{DOKLADY EARTH SCIENCES}},
Year = {{2017}},
Volume = {{476}},
Number = {{2}},
Pages = {{1203-1206}},
Month = {{OCT}},
Abstract = {{We distinguish the classes of Subpolar Mode Waters (SPMWs), calculate
   their long-term characteristics, and analyze their interannual and
   long-term variability based on data from the transatlantic section along
   59.5A degrees N measured in 2002-2016. The branches of the North
   Atlantic Current are horizontal boundaries of the SPMW classes. Modern
   cooling and freshening of SPMW cores are revealed due to the increased
   winter atmospheric circulation over the North Atlantic and decline of
   the Arctic ice cover. In 2010-2016, the potential temperature of the
   SPMW core in the Iceland Basin dropped by 2.12A degrees C, while its
   salinity decreased by 0.23 psu.}},
