@article{ ISI:000396557000007,
Author = {Majstorovic, Gorica},
Title = {{Cosmopolitan critique and the ``Atlantic{''} Arlt}},
Journal = {{ATLANTIC STUDIES-GLOBAL CURRENTS}},
Year = {{2017}},
Volume = {{14}},
Number = {{1}},
Pages = {{99-111}},
Abstract = {{This essay examines Roberto Arlt's journalistic account of visits to
   peripheral Spanish geographies on the Atlantic, Morocco and Galicia, by
   focusing not only on travel's engagement with mobility and storytelling,
   but also with the cultural capital the traveler hopes to gain at home,
   and on the national stage. By referencing both the Atlantic and also
   Arlt's North African travels along the Mediterranean, the essay aims at
   opening up lines of interconnectivity, interdependency, and
   inter-relational flows to and from the Atlantic on a more global scale.
   It contributes to the repositioning of Atlantic discourses (and their
   respective geopolitics) and in doing so examines the role of the uneven
   modernity that informed Arlt's travel gaze. Coining the term ``travel as
   technique{''} as a critical notion, it refers to the praxis of writing
   travel alongside history, a praxis through which Arlt documented Spanish
   political turbulence and the rivalry between global colonial powers in
   1935. Departing from a critical cosmopolitanism stance (Homi Bhabha,
   Pheng Cheah, et al.), it follows the ``Hispanic relocation{''} (Joseba
   Gabilondo) and ``engages the Atlantic{''} (Benita Sampedro Vizcaya),
   from a global perspective that, in various formulations of Arlt's
   cosmopolitan critique, includes class-consciousness and its connectivity
   with the Mediterranean and African geopolitical flows. The critical
   examination of the elite cosmopolitan ``genteel tradition{''} (David
   Vinas) in Arlt's Galician travelogue draws on the attention the
   Argentine author paid to relations of social class and power in the
   realm of Atlantic politics, but also in the realm of cultural production
   and consumption on both sides of the ocean.}},
Publisher = {{ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD}},
