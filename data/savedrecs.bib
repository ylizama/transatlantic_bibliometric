@article{ ISI:000394953200002,
Author = {Appleby, John},
Title = {{Daniel {[}De] Foe's Virginia venture: Mutiny and indiscipline at sea
   during the 1680s and 1690s}},
Journal = {{INTERNATIONAL JOURNAL OF MARITIME HISTORY}},
