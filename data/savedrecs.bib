@article{ ISI:000400951200008,
Author = {Goulet, Andrea},
Title = {{Tooth Decay Edgar Allan Poe and the Neuro-deca{''}dent{''}isme of
   Villiers and Huysmans}},
Journal = {{NINETEENTH-CENTURY FRENCH STUDIES}},
Year = {{2017}},
Volume = {{45}},
Number = {{3-4}},
Pages = {{198-218}},
Month = {{SPR-SUM}},
Abstract = {{This essay studies dental motifs in fin-de-siecle decadent fiction,
   including Villiers de l'Isle-Adam's L'Eve future and especially
   Huysmans's A rebours, in the double context of neurological medical
   discourses (Beard, Charcot) and the transatlantic history of dentistry
   in France and the United States. By way of Poe's and Redon's
   neuropathologicat visions, tooth-rot in Villiers and Huysmans becomes
   revalued as part of a modern aesthetic sensibility, a refined reaction
   against the effects of American consumerism and base materiality. From
   the supplemental artifice of dentures to the fleur phenique used to
   treat neurasthenia and neurosyphilis, teeth embody decadent reflections
   on corruptibility, nervosisme, and art.}},
