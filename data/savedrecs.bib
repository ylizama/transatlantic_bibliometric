@article{ ISI:000394529800003,
Author = {Gonzalez, Carlos Varon},
Title = {{Ruben Dario, Fascist? Francoist readings of Modernismo}},
Journal = {{BULLETIN OF HISPANIC STUDIES}},
Year = {{2017}},
Volume = {{94}},
Number = {{2}},
Pages = {{163-182}},
Abstract = {{This article traces the presence of Nicaraguan poet Ruben Dario in
   Spanish literary criticism and film (1939-1944). The significance of his
   poetry, cosmopolitanism, nationality, race and modernist aesthetics pose
   a challenge to an early Francoist understanding of culture. Both
   literary criticism and propaganda film resorted to him as a way to
   legitimize and advance their respective aesthetic and political agendas.
   The struggle to rethink his position vis-a-vis Spanish culture reveals a
   series of factions and anxieties - aesthetic, historical, and political
   in nature - pertaining to the peninsular cultural field of the 1940s.
   Ultimately, these positionings highlight Dario's importance as a
   signifier of modernity in Transatlantic Hispanic culture.}},
