@article{ ISI:000262635700006,
Author = {Mahncke, Dieter},
Title = {{The United States, Germany and France: Balancing Transatlantic Relations}},
Journal = {{BRITISH JOURNAL OF POLITICS \& INTERNATIONAL RELATIONS}},
Year = {{2009}},
Volume = {{11}},
Number = {{1}},
Pages = {{79-93}},
Month = {{FEB}},
Abstract = {{Relations between the United States, France and Germany significantly
   determine the content and structure of the broader transatlantic
   relationship. Relations improved after the changes of government in
   Germany in 2005 and France in 2007, and they can be expected to improve
   further after the change of administration in the United States in 2009.
   Washington is likely to pay more attention to its European allies while
   these may become more involved in issues beyond Europe's borders.
   Sharing leadership will be the challenge for the United States, while
   bringing more intellectual and material input will be the challenge for
   the Europeans. At the same time, France and Germany will develop and
   maintain independent views. But if partnership is the aim on both sides
   of the Atlantic, a more promising relationship can be expected even
   where differences exist.}},
