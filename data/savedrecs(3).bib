@article{ ISI:000257398700022,
Author = {Gore, Mauvis A. and Rowat, David and Hall, Jackie and Gell, Fiona R. and
   Ormond, Rupert F.},
Title = {{Transatlantic migration and deep mid-ocean diving by basking shark}},
Journal = {{BIOLOGY LETTERS}},
Year = {{2008}},
Volume = {{4}},
Number = {{4}},
Pages = {{395-398}},
Month = {{AUG 23}},
Abstract = {{Despite being the second largest fish, basking sharks (Cetorhinus
   maximus) have been assumed to remain in discrete populations. Their
   known distribution encompasses temperate continental shelf areas, yet
   until now there has been no evidence for migration across oceans or
   between hemispheres. Here we present results on the tracks and behaviour
   of two basking sharks tagged off the British Isles, one of which
   released its tag off Newfoundland, Canada. During the shark's transit of
   the North Atlantic, she travelled a horizontal distance of 9589 km and
   reached a record depth of 1264 m. This result provides the first
   evidence for a link between European and American populations and
   indicates that basking sharks make use of deep-water habitats beyond the
   shelf edge.}},
