@article{ ISI:000265733100004,
Author = {Sarafanov, A. A. and Sokov, A. V. and Falina, A. S.},
Title = {{Warming and salinification of Labrador Sea Water and deep waters in the
   subpolar North Atlantic at 60A degrees N in 1997-2006}},
Journal = {{OCEANOLOGY}},
Year = {{2009}},
Volume = {{49}},
Number = {{2}},
Pages = {{193-204}},
Month = {{APR}},
Abstract = {{A quantitative estimate of the temperature and salinity variations in
   the Labrador Sea Water (LSW), the Iceland-Scotland Overflow Water
   (ISOW), and the Denmark Strait Overflow Water (DSOW) is given on the
   basis of the analysis of repeated observations over a transatlantic
   section along 60A degrees N in 1997, 2002, 2004, and 2006. The changes
   distinguished in the research evidence strong warming and salinification
   in the layers of the Labrador Sea Water and deep waters at the latitude
   of the section. The maximum increments of the temperature (+0.35A
   degrees C) and salinity (+0.05 psu) were found in the Irminger Basin in
   the core of the deep LSW, whose convective renewal in the Labrador Sea
   stopped in the mid-1990s. The long-term freshening of the ISOW, which
   started in the mid-1960s, changed in the mid-1990s to a period of
   intense stable warming and salinification of this water. By 2005, the
   salinity in the core of the ISOW in the Iceland Basin increased to the
   values (similar to 34.99 psu) characteristic of the mid-1970s. In 2002,
   the warming ``signal{''} of the ISOW reached the Irminger Basin. From
   1997 to 2006, the warming and salinification of the columns of the
   Labrador Sea Water and deep waters became as high as 0.2A degrees C and
   0.03 psu, respectively. The character of the long-term variations in the
