@article{ ISI:000209059500004,
Author = {Bugajski, Janusz},
Title = {{Atlanticism without Orientation The United States, Obama, and Europe}},
Journal = {{OSTEUROPA}},
Year = {{2011}},
Volume = {{61}},
Number = {{1}},
Pages = {{31+}},
Month = {{JAN}},
Abstract = {{Europe's relevance in American foreign policy has declined. The
   foundation of the transatlantic relationship during the East-West
   conflict the joint creation of liberty, security, democracy, and
   prosperity has become brittle. In political circles and among the
   general public on both sides of the Atlantic, irritations about one
   another shape the picture. Even the East Central Europeans are unsettled
   by U.S. President Barak Obama's new policy toward Russia and the lack of
   clarity as to whether the United States will remain at their side. A new
   vision is needed to revitalise transatlantic relations. But there is a
   shortage of common ground in content and of politicians ready to involve
   themselves to this end.}},
