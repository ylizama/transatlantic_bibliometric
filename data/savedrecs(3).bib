@article{ ISI:000260467600002,
Author = {Feys, Torsten},
Title = {{PREPAID TICKETS TO THE NEW WORLD: THE NEW YORK CONTINENTAL CONFERENCE
   AND TRANSATLANTIC STEERAGE FARES 1885-1895}},
Journal = {{REVISTA DE HISTORIA ECONOMICA}},
Year = {{2008}},
Volume = {{26}},
Number = {{2}},
Pages = {{173-204}},
Month = {{FAL}},
Abstract = {{This article surveys the situation of the shipping cartels (conferences)
   which regulated transatlantic migrant transport from the European
   Continent to the United States. The focus of the article is to identify
   the internal and external pressures underlying these agreements and the
   strategies employed to neutralize these pressures. The author reaches
   the conclusion that a pool agreement was essential for the effectiveness
   of the Conference, which was both a means of horizontal integration
   regulating the competition between shipping companies and a means of
   vertical integration to gain control over the transatlantic migrant
   agent network selling the ocean passage tickets. The author also
   correlates the efficiency of the agreements with steerage fares and
   migration costs.}},
