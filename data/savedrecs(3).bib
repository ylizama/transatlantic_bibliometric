@article{ ISI:000209796200032,
Author = {Strat, Ioana},
Title = {{The Common Foreign and Security Policy post-Treaty of Lisbon
   Implications for the transatlantic cooperation in the field of security}},
Journal = {{TRANSYLVANIAN REVIEW}},
Year = {{2011}},
Volume = {{20}},
Number = {{2, 2}},
Pages = {{813-824}},
Abstract = {{The scope of this paper falls into the sphere of International Relations
   and European security studies through an in-depth analysis of the EU's
   post-Lisbon foreign policy against the prospects of transatlantic
   security cooperation. This study aims to investigate the Treaty of
   Lisbon's institutional innovations in the area of common foreign and
   security policy, arguing that there is a discrepancy between, on the one
   hand, the expectations of global security actors, namely the US and
   NATO, concerning the development by the EU of a coherent external
   identity, and, on the other hand, the end results of the EU's
   institutional reform in terms of representative public figures,
   decision-making process and the interaction between the supranational
   and intergovernmental levels. The contribution of this paper consists in
   a few recommendations for the enhancement of security cooperation
   between the EU and NATO post-Treaty of Lisbon in order to help
   consolidate the transatlantic strategic partnership and to better
   systemize EU's global role.}},
