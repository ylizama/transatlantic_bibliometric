@article{ ISI:000294393300007,
Author = {Jackson, Antoinette T.},
Title = {{Shattering Slave Life Portrayals: Uncovering Subjugated Knowledge in US
   Plantation Sites in South Carolina and Florida}},
Journal = {{AMERICAN ANTHROPOLOGIST}},
Year = {{2011}},
Volume = {{113}},
Number = {{3}},
Pages = {{448-462}},
Month = {{SEP}},
Abstract = {{There is an ongoing dialogue about slavery that is moored to
   ideological, social, and physical remnants of plantations. Scholars are
   active participants in creating and interpreting representations of
   postbellum plantations as public heritage sites that shape national
   memory. What tools and theoretical approaches can inform how we
   interpret, analyze, and represent characterizations of plantation life
   today? In this article, I talk about the historical moment in which
   plantations existed (transatlantic slavery), descendent interpretations,
   and ways in which descendent memories instruct reconfiguration of
   systems of categorization today. Research conducted in the three
   postbellum plantation communities in the U.S. South that are referenced
   in this analysis-Boone Hall and Snee Farm in South Carolina and Kingsley
   Plantation in Florida-inform this discussion.}},
