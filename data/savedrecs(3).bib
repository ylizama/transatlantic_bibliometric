@article{ ISI:000272794600011,
Author = {Guillemaud, T. and Beaumont, M. A. and Ciosi, M. and Cornuet, J-M and
   Estoup, A.},
Title = {{Inferring introduction routes of invasive species using approximate
   Bayesian computation on microsatellite data}},
Journal = {{HEREDITY}},
